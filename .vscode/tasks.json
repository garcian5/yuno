{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "serve-ionic",
      "type": "shell",
      "command": "ionic serve",
      "isBackground": true,
      "problemMatcher": {
        "pattern": [
          {
            "regexp": ".",
            "file": 1,
            "location": 2,
            "message": 3
          }
        ],
        "background": {
          "activeOnStart": true,
          "beginsPattern": "Starting dev server",
          "endsPattern": "Compiled successfully"
        }
      }
    }
  ]
}
